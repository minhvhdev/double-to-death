{
  "usesTypeScript": true,
  "usesCssModule": true,
  "usesStyledComponents": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/common/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false,
      "customTemplates": {
        "style": "src/grc-templates/components/TemplateName.css.ts",
        "component": "src/grc-templates/components/TemplateName.tsx"
      }
    },
    "page": {
      "path": "src/common/components/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false,
      "customTemplates": {
        "style": "src/grc-templates/pages/TemplateName.css.ts",
        "component": "src/grc-templates/pages/TemplateName.tsx"
      }
    }
  }
}
